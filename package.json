    "@radix-ui/react-slot": "^1.1.2",
    "@turf/turf": "^7.2.0",
    "@types/react-map-gl": "^6.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "envobj": "^2.0.2",
    "h3-js": "^4.1.0",
